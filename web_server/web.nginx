server {
    listen      80;
    server_name www.taidimall.com;
    location / {
        proxy_pass http://127.0.0.1:8001/;
        proxy_set_header Host $host:$server_port;
    }
}

server {
    listen      80;
    server_name api.taidimall.com;
    location / {
        proxy_pass http://127.0.0.1:8002/;
        proxy_set_header Host $host:$server_port;
    }
}
